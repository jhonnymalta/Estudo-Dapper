@model UsuarioModel
@{
    ViewData["Title"] = "Novo Usuarido";
}
<h1>Cria novo usuario  </h1>
<form asp-controller="Usuario" asp-action="CriarView">
    <div  class="mb-3">
        <label asp-for="Nome" class="form-label"></label>
        <input type="text" asp-for="Nome"   class="form-control"/>
    </div>
    <div  class="mb-3">
        <label asp-for="Email" class="form-label"></label>
        <input type="email" asp-for="Email"   class="form-control"/>
    </div>
     <div  class="mb-3">
        <label asp-for="Sexo" class="form-label"></label>
        <input type="text" asp-for="Sexo"   class="form-control"/>
    </div>
     <div  class="mb-3">
        <label asp-for="RG" class="form-label"></label>
        <input type="text" asp-for="RG"  class="form-control"/>
    </div>
     <div  class="mb-3">
        <label asp-for="CPF" class="form-label"></label>
        <input type="text" asp-for="CPF"  class="form-control"/>
    </div>
     <div  class="mb-3">
        <label asp-for="NomeMae" class="form-label"></label>
        <input type="text" asp-for="NomeMae" class="form-control"/>
    </div>
    
     <div  class="mb-3">
        <label asp-for="SituacaoCadastro" class="form-label"></label>
        <input type="text" asp-for="SituacaoCadastro"  class="form-control"/>
    </div>   
     <div  class="mb-3">
        <label asp-for="DataCadastro" class="form-label"></label>
        <input type="datetime-local" asp-for="DataCadastro"  class="form-control"/>
    </div>
  

    <a asp-action="Index" class="btn btn-warning">Voltar</a>
    <button class="btn btn-primary" type="submit" >Criar</button>
</form>
